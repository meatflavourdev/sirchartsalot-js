{
  "name": "sirchartsalot",
  "version": "0.4.0",
  "description": "A simple javascript/CSS bar chart library created as part of the Lighthouse Labs Web Development Bootcamp prep course.",
  "author": "Jeremy Dombrowski",
  "main": "docs/index.html",
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "dev": "npm-run-all --parallel start watch",
    "start": "browser-sync start -s -f . --no-notify --host $LOCAL_IP --port 10001"
  },
  "devDependencies": {
    "eslint": "^3.19.0",
    "eslint-config-lighthouselabs": "lighthouse-labs/eslint-config-lighthouselabs",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.16.2",
    "rollup-plugin-css-bundle": "^1.0.4"
  },
  "dependencies": {
    "@svgdotjs/svg.js": "^3.0.16",
    "browser-sync": "^2.27.11",
    "domready": "^1.0.8"
  }
}
